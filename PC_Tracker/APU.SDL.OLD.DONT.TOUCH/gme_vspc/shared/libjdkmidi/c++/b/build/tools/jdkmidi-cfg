#!/bin/bash
for i in $*; do
 case $i in 
  (--ldflags) echo -n "   -L/Users/bazz/Projects/libjdkmidi-read-only/c++/b/install/lib ";;
  (--ldlibs) echo -n "  -ljdkmidi ";;
  (--cflags) echo -n "   -I/Users/bazz/Projects/libjdkmidi-read-only/c++/b/install/include    ";;
  (--cxxflags) echo -n "   -I/Users/bazz/Projects/libjdkmidi-read-only/c++/b/install/include    ";;
  (--cppflags) echo -n " ";;
  (--mflags) echo -n "   -I/Users/bazz/Projects/libjdkmidi-read-only/c++/b/install/include    ";;
  (--mmflags) echo -n "   -I/Users/bazz/Projects/libjdkmidi-read-only/c++/b/install/include    ";;
 esac
done
echo
